{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "hh.design",
      "protocol": "inspector",
      "program": "${workspaceRoot}/node_modules/.bin/nuxt",
      "stopOnEntry": true,
      "args": ["dev"],
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "env": {
          "NODE_ENV": "development",
          "DEBUG": "nuxt:*,app"
      },
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Port",
      "address": "localhost",
      "port": 9229
    }
  ]
}